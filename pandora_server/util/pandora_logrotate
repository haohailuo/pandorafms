/var/log/pandora/pandora_server.log 
/var/log/pandora/pandora_server.error {
	weekly
	missingok
	size 128144
	rotate 3
	maxage 15
	compress
	notifempty
	create 644 root root
	sharedscripts
	postrotate
	/etc/init.d/pandora_server restart
	endscript
}

/var/log/pandora/pandora_snmptrap.log {
	daily
	missingok
	size 150000
	rotate 1
	maxage 7
	notifempty
	create 644 root root
}
